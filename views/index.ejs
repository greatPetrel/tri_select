<!doctype html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
    <title>地区选择插件</title>
    <link rel="stylesheet" type="text/css" href="css/tri_select.css">
    <script language="javascript" type="text/javascript" src="js/jquery.min.js"></script>
    <script language="javascript" type="text/javascript" src="data/location.js"></script>
    <script language="javascript" type="text/javascript" src="js/tri_select_jsdata.js"></script>
</head>

<body>
    <div class="multi_level_select_box">
        <div class="select_wrap_box">
            <div class="fake_select " level='1'>
                <div class="select_text_area">请选择</div>
                <img class="fake_arrow" src="img/images.png" alt="箭头">
            </div>
            <ul class="tri_select_item" id="s_level_1" name="s_level_1">
            </ul>
        </div>
        <div class="select_wrap_box">
            <div class="fake_select  " level='2'>
                <div class="select_text_area">请选择</div>
                <img class="fake_arrow" src="img/images.png" alt="箭头">
            </div>
            <ul class="tri_select_item" id="s_level_2" name="s_level_2">
            </ul>
        </div>
        <div class="select_wrap_box">
            <div class="fake_select  " level='3'>
                <div class="select_text_area">请选择</div>
                <img class="fake_arrow" src="img/images.png" alt="箭头">
            </div>
            <ul class="tri_select_item" id="s_level_3" name="s_level_3">
            </ul>
        </div>
    </div>
    <div>
        <button id="get_xml" style="cursor: pointer;">请求xml文件</button>
        <br>
        <button id="get_json" style="cursor: pointer; float:right;">
            请求json文件
        </button>
    </div>
</body>

</html>
